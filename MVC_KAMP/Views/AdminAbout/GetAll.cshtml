@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "GetAll";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<div class="table-responsive">
    <table class="table align-middle">
        <thead>
            <tr class="align-bottom">
                <th>#</th>
                <th>Hakkında Başlığı</th>
                <th>Durumu</th>
                <th>İşlemler</th>

            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td>@item.AboutId</td>
                    <td>Başlık</td>
                    

                </tr>
            }

        </tbody>
    </table>
    <a href="/AdminAbout/Create" class="btn btn-info" data-bs-toggle="modal"data-bs-target="#aboutCreateModal">Başlık Ekle</a>
</div>


@Html.Action("PartialCreate", "AdminAbout")